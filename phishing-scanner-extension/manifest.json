{
  "manifest_version": 3,
  "name": "Phishing Scanner",
  "version": "1.0",
  "description": "Scan URLs to check for phishing threats.",
  "action": {
    "default_popup": "scanner.html",
    "default_icon": {
      "16": "images/logo2.jpg",
      "48": "images/logo3.jpg",
      "128": "images/logo4.jpg"
    }
  },
  "permissions": [
    "activeTab"
  ]
}

